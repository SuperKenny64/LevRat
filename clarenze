# -*- coding: utf-8 -*-

loadpath = r'C:\Users\...'
savepath = r'C:\Users\...'

 

data = []

 

import csv

with open(loadpath, mode = 'r') as file:

  wert = csv.reader(file)
    for row in wert:
      data = row[3]
      
      with open(savepath, mode='w') as file1:
        writer = csv.writer(file, delimiter=',', quotechar='"', quoting=csv.QUOTE_MINIMAL)
        writer.writerow(data)
        data = ""

file.close()
file1.close()
